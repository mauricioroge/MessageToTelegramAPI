{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQConfiguration": {
    "Username": "USERNAME",
    "Secret": "PASSWORD",
    "HostName": "localhost",
    "VirtualHost": "/",
    "DispatcherAsync": true,
    "TopologyRecovery": true,
    "AutomaticRecovery": true,
    "NetworkRecoverySeconds": 5
  },
  "UserMessagesQueueConfiguration": {
    "QueueName": "UserMessagesQueue",
    "RoutingKey": "mt7",
    "Durable": true,
    "ExchangeName": "UserMessagesExchange",
    "ExchangeType": "direct",
    "Exclusive": false,
    "AutoDelete": false
  },
  "TelegramBotConfiguration": {
    "Token": "INSERT_YOUR_BOT_TOKEN_HERE"
  },
  "ServerMessagesQueueConfiguration": {
    "QueueName": "ServerMessagesQueue",
    "RoutingKey": "mt7",
    "Durable": true,
    "ExchangeName": "ServerMessagesQueue",
    "ExchangeType": "direct",
    "Exclusive": false,
    "AutoDelete": false
  }
}
